<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="OdtwarzaczMuzyki.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OdtwarzaczMuzyki"
    Title="OdtwarzaczMuzyki"
    FlyoutBehavior="Disabled"
    Background="#274690">

    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:PlaylistPage}"
        Route="PlaylistPage" />
    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:CurrentlyPlaying}"
        Route="CurrentlyPlaying" />
    
    

</Shell>
